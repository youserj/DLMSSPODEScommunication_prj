    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current Test File",
            "type": "debugpy",
            "request": "launch",
            "program": "-m",
            "args": [
                "unittest",
                "${file}",
                "-v"
            ],
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "Debug test_open_close",
            "type": "debugpy",
            "request": "launch",
            "program": "-m",
            "args": [
                "unittest",
                "test.test_Network.TestType.test_open_close",
                "-v"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        },
        {
            "name": "Debug Ble",
